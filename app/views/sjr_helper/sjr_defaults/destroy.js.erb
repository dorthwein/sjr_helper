var elements = $('[data-sjr-object="true"][data-sjr-id="<%= j(object.id.to_s) %>"][data-sjr-class="<%= j(object.class.to_s) %>"]')
elements.each(function(index, element){
	$(element).hide('puff', 500, function(){
		$(element).remove()
	})
})

